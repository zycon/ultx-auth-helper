{
   "name": "Ultimatix Login Helper",
   "version": "1.0",
   "description": "No more waiting for 2fa!",
   "default_locale": "en",
   "manifest_version":2,
   "browser_action": {
      "default_icon": {
         "38": "apps/ultimatix.png"
      },
      "default_title": "Ultimatix Login helper"
   },
   "background": {
     "scripts": [
       "login.js"
     ],
     "persistent": true
   },
   "icons": {
      "128": "apps/ultimatix.png" 
   },
   "permissions": [
     "https://*/*",
     "http://*/*",
     "tabs"
   ]
 }